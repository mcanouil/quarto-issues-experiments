{
	"name": "Quarto v1.4.534",
	"image": "ghcr.io/mcanouil/quarto-codespaces:latest",
	"remoteUser": "vscode",
	"features": {
		"ghcr.io/rocker-org/devcontainer-features/quarto-cli:1": {
			"version": "1.4.534",
			"installTinyTex": "false",
			"installChromium": "false"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"quarto.quarto",
				"REditorSupport.r"
			],
			"settings": {
				"r.rterm.option": [
					"--no-save",
					"--no-restore-data",
					"--quiet"
				],
				"r.useRenvLibPath": true
			}
		}
	}
}
